{
  "firehoseOffline": "env://GITHUB_FIREHOSE_OFFLINE",

  "sharedSecret": "env://GITHUB_WEBHOOK_SHARED_SECRET",

  "runtimeMinutes": "env://GITHUB_WEBHOOKS_RUNTIME_MINUTES",
  "parallelism": "env://GITHUB_WEBHOOKS_MAX_PARALLELISM",
  "emptyQueueDelaySeconds": "env://GITHUB_WEBHOOKS_EMPTY_DELAY_SECONDS",

  "provider": "env://GITHUB_WEBHOOKS_PROVIDER",

  "serviceBus": {
    "connectionString": "env://GITHUB_WEBHOOKS_SERVICEBUS_CONNECTIONSTRING",
    "queue": "env://GITHUB_WEBHOOKS_SERVICEBUS_QUEUE",
    "useEntraAuthentication": "env://GITHUB_WEBHOOKS_SERVICEBUS_USE_ENTRA_AUTH?trueIf=1",
    "endpoint": "env://GITHUB_WEBHOOKS_SERVICEBUS_ENDPOINT"
  },

  "azureQueues": {
    "account": "env://GITHUB_WEBHOOKS_QUEUES_ACCOUNT",
    "sas": "env://GITHUB_WEBHOOKS_QUEUES_SAS",
    "queue": "env://GITHUB_WEBHOOKS_QUEUES_QUEUE"
  }

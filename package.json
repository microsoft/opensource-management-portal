  "scripts": {
    "build": "tsc",
    "build:oss": "tsc --project ./tsconfig.oss.json",
    "build:frontend": "pushd frontend && npm run build && popd",
    "build:all": "tsc && pushd frontend && npm run build && popd",
    "dev:local": "cross-env DEBUG=startup EXIT_IMMEDIATELY=1 node ./dist/scripts/localEnvironment.js",
    "dev:react-build": "pushd frontend && npm run build && popd && DEBUG=startup node ./dist/bin/www",
    "find-deadcode": "ts-prune",
    "find-circular-dependencies": "npx madge --circular --extensions ts --exclude 'dist|frontend' .",
    "fix:js": "eslint --fix .",
    "fix:md": "markdownlint-cli2 --config ./.config/.markdownlint-cli2.jsonc --fix \"**/*.md\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix": "npm run fix:js && npm run fix:md && npm run fix:prettier",
    "lint:js": "eslint . --config eslint.config.mjs",
    "lint:json": "echo OK",
    "lint:md": "markdownlint-cli2 --config ./.config/.markdownlint-cli2.jsonc \"**/*.md\"",
    "lint:spell": "cspell --config ./.cspell.json --no-progress \"**\"",
    "lint": "npm run lint:json && npm run lint:js && npm run lint:md && npm run lint:spell",
    "outdated": "npm outdated; pushd frontend; npm outdated; popd",
    "install:all": "npm install && pushd frontend && npm install && popd",
    "prepare": "husky || echo No Husky.",
    "start-4000": "cross-env PORT=4000 FRONTEND_MODE=proxied DEBUG=startup node ./dist/bin/www",
    "start-in-container": "node ./bin/www",
    "start": "node ./dist/bin/www",
    "test:ci": "vitest run",
    "test": "vitest run"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown",
      "cspell --no-must-find-files"
    ],
    "*.{js,ts}": "eslint --fix",
    "*.md": "markdownlint-cli2 --fix --config ./.config/.markdownlint-cli2.jsonc"
  },
  "_begin_app_configuration_": 0,
  "//": "- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -",
  "painlessConfigObjectPackages": [
    "./config"
  ],
  "continuousDeployment": {
    "build": "__Build_BuildNumber__",
    "builder": "__Build_Builder__",
    "release": "",
    "branchName": "__Build_BranchName__",
    "commitId": "__Build_SourceVersion__"
  },
  "static-react-folder": "frontend",
  "//.": "- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - ",
  "_end_app_configuration_": 0,
  "_begin_microsoft_internal_": 0,
  "_end_microsoft_internal_": 0,
